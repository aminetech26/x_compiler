program langageX.

struct {
    etudiant = {
        int id,
        str nom
        }.
}
var{
    int A.
    etudiant mahdi.
    bool boolean.
}

start 
    A = 5.
    
    check (A >= 7) {
        boolean = true.
    } else {
        mahdi->id = 2025.
        mahdi->nom = "Mahdi".
    }  
    cycle (A from 1 to 5 by 1) {
        mahdi->id = mahdi->id + 5.
    }
    
finish